! Xft settings ---------------------------------------------------------
Xft.autohint:   0
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.embolden:   true

URxvt*termName: xterm-256color
URxvt.letterSpace: -1
URxvt.lineSpace: 0
URxvt.scrollBar:  False
#URxvt.font:         xft:DejaVu Sans Mono:pixelsize=13:antialias=true
#URxvt.boldFont:     xft:DejaVu Sans Mono:Bold:pixelsize=13:antialias=true
#URxvt.font:     xft:monospace:size=10
#URxvt.boldFont: xft:monospace:bold:size=10
URxvt.font:     xft:inconsolata:pixelsize=13
URxvt.boldFont: xft:inconsolata:bold:pixelsize=13
#URxvt.font:     xft:bitstream vera sans mono:size=10:antialias=true
#URxvt.boldFont: xft:bitstream vera sans mono:bold:size=10:antialias=true


URxvt*foreground: #cbc3b5
URxvt*background: #282834
URxvt*cursorColor: #b1170b
URxvt*cursorColor2: #ffffff
URxvt*highlightColor: #c1deff
URxvt*highlightTextColor: #000000
URxvt*colorBD: #ff938f

! black
URxvt*color0  : #000000
URxvt*color8  : #686868
! red
URxvt*color1  : #c91b00
URxvt*color9  : #ff938f
! green
URxvt*color2  : #00c200
URxvt*color10 : #5ffa68
! yellow
URxvt*color3  : #c7c400
URxvt*color11 : #fffc67
! blue
URxvt*color4  : #006de1
URxvt*color12 : #3d3d5d
! magenta
URxvt*color5  : #ca30c7
URxvt*color13 : #ff77ff
! cyan
URxvt*color6  : #00c5c7
URxvt*color14 : #60fdff
! white
URxvt*color7: #c7c7c7
URxvt*color15 : #ffffff

URxvt.iso14755: false
URxvt.iso14755_52: false

#URxvt.keysym.Mod4-v: eval:paste_clipboard
#URxvt.keysym.Mod4-c: eval:selection_to_clipboard

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
